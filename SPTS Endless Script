local pageMisc = hub:addPage("Misc", 5012544693)

local sectionMisc = pageMisc:addSection("discord.gg/MqsE3Wn7Fr by ZacaUltim")

sectionMisc:addKeybind("UI Toggle Keybind", Enum.KeyCode.RightShift, function()
    print("Activated Keybind")
    hub:toggle()
    end, function()
    print("Changed Keybind")
end)

-- load
hub:SelectPage(hub.pages[1], true)

--MAIN CODE--
while true do
    if getstrength == true then
        local A_1 = 
        {
            [1] = "Add_FS_Request"
        }
        local Event = game:GetService("ReplicatedStorage").RemoteEvent
        Event:FireServer(A_1)
    end

    if getdurability == true then
        local A_1 = 
        {
            [1] = "+BT1"
        }
        local Event = game:GetService("ReplicatedStorage").RemoteEvent
        Event:FireServer(A_1)
    end

    if getspeed == true then
        local A_1 = 
        {
            [1] = "Add_MS_Request"
        }
        local Event = game:GetService("ReplicatedStorage").RemoteEvent
        Event:FireServer(A_1)
    end

    if getjump == true then
        local A_1 = 
        {
            [1] = "Add_JF_Request"
        }
        local Event = game:GetService("ReplicatedStorage").RemoteEvent
        Event:FireServer(A_1)
    end

    if autobuy == true then
        if autobuystat == "Strength" then
            local A_1 = 
            {
                [1] = "StatMultiplier", 
                [2] = "FS"
            }
            local Event = game:GetService("ReplicatedStorage").RemoteEvent
            Event:FireServer(A_1)
        end

        if autobuystat == "Toughness" then
            local A_1 = 
            {
                [1] = "StatMultiplier", 
                [2] = "BT"
            }
            local Event = game:GetService("ReplicatedStorage").RemoteEvent
            Event:FireServer(A_1)
        end

        if autobuystat == "Speed" then
            local A_1 = 
            {
                [1] = "StatMultiplier", 
                [2] = "MS"
            }
            local Event = game:GetService("ReplicatedStorage").RemoteEvent
            Event:FireServer(A_1)
        end

        if autobuystat == "Jump Height" then
            local A_1 = 
            {
                [1] = "StatMultiplier", 
                [2] = "JF"
            }
            local Event = game:GetService("ReplicatedStorage").RemoteEvent
            Event:FireServer(A_1)
        end

        if autobuystat == "Psychic Power" then
            local A_1 = 
            {
                [1] = "StatMultiplier", 
                [2] = "PP"
            }
            local Event = game:GetService("ReplicatedStorage").RemoteEvent
            Event:FireServer(A_1)
        end
    end
    wait()
end
