--UI MENU CODE--
getgenv().getstrength = false
getgenv().gettoughness = false
getgenv().getspeed = false
getgenv().getjump = false
getgenv().autobuy = false
getgenv().autobuystat = nil

-- init
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/GreenDeno/Venyx-UI-Library/main/source.lua"))()
local hub = library.new("STPS Endless Hub", 5013109572)

-- themes
local themes = {
Background = Color3.fromRGB(24, 24, 24),
Glow = Color3.fromRGB(0, 0, 0),
Accent = Color3.fromRGB(10, 10, 10),
LightContrast = Color3.fromRGB(20, 20, 20),
DarkContrast = Color3.fromRGB(14, 14, 14),  
TextColor = Color3.fromRGB(255, 255, 255)
}

-- first page
local pageStats = hub:addPage("Stats", 5012544693)
local sectionStatsAF = pageStats:addSection("Autofarm")

sectionStatsAF:addButton("Info: Psychic Power is NOT autofarmable. Farm it normally,", function()
end)

sectionStatsAF:addButton("But it will interfere with Speed and Jump farming.", function()
end)

sectionStatsAF:addButton("--------------------------------------------------------------", function()
end)

sectionStatsAF:addToggle("AF Strength", false, function(value)
    getgenv().getstrength = value
    print("Toggled AF Strength to", value)
end)

sectionStatsAF:addToggle("AF Toughness", false, function(value)
    getgenv().getdurability = value
    print("Toggled AF Toughness to", value)
end)

sectionStatsAF:addToggle("AF Speed", false, function(value)
    getgenv().getspeed = value
    print("Toggled AF Speed to", value)
end)

sectionStatsAF:addToggle("AF Jump Height", false, function(value)
    getgenv().getjump = value
    print("Toggled AF Jump Height to", value)
end)

sectionStatsAF:addButton("", function()
end)

sectionStatsAF:addToggle("Auto Buy / Upgrade", false, function(value)
    getgenv().autobuy = value
    print("Toggled Auto Buy / Upgrade to", value)
end)

sectionStatsAF:addDropdown("Choose Stat to Upgrade", {"Strength", "Toughness", "Speed", "Jump Height", "Psychic Power"}, function(text)
    getgenv().autobuystat = text
    print("Set Auto Buy Stat to", text)
end)

